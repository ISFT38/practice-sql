# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                             controllers.Application.index
POST        /login                        controllers.UserController.login
POST        /logout                       controllers.UserController.logout
POST        /createUser                   controllers.UserController.create
POST        /changePassword               controllers.UserController.changePassword
POST        /createChallenge              controllers.ChallengeController.create
        
# Prefix must match `play.assets.urlPrefix`
GET        /assets/*file                 controllers.Assets.at(file)
GET        /versionedAssets/*file        controllers.Assets.versioned(path="/public", file: Asset)
